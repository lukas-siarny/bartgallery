tatus === \"fulfilled\"){\r\n            onCategoryStatusChange();\r\n        }\r\n    }\r\n\r\n    return (\r\n        <>  \r\n            <figure className=\"card card--existing\">\r\n                <div className=\"card__wrapper\">\r\n                    <div className=\"card__delete-image\">\r\n                        <i className=\"far fa-trash-alt\" onClick={handleDeleteModalOpen}></i>\r\n                    </div> \r\n                    <div className=\"card__image-wrapper\" onClick={onPhotoClick}>   \r\n                        {renderedImg}\r\n                    </div>   \r\n                </div>     \r\n            </figure>  \r\n            \r\n            {deleteModalVisibility &&\r\n                <Modal onModalClose={onDeleteModalClose}>\r\n                    {renderedModalContent}\r\n                </Modal>\r\n            }\r\n        </>\r\n    )\r\n}\r\n\r\nexport default PhotoCard;","C:\\xampp\\htdocs\\react\\bart\\src\\components\\singlecategory\\SingleCategoryPhotosGrid.js",[],"C:\\xampp\\htdocs\\react\\bart\\src\\api-client.js",[],"C:\\xampp\\htdocs\\react\\bart\\src\\components\\general\\CardEmpty.js",[],"C:\\xampp\\htdocs\\react\\bart\\src\\components\\general\\CardAdd.js",[],"C:\\xampp\\htdocs\\react\\bart\\src\\components\\general\\Navbar.js",[],"C:\\xampp\\htdocs\\react\\bart\\src\\components\\Footer.js",[],{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","severity":1,"message":"113","line":58,"column":8,"nodeType":"114","endLine":58,"endColumn":16,"suggestions":"115"},{"ruleId":"116","severity":1,"message":"117","line":95,"column":64,"nodeType":"118","messageId":"119","endLine":95,"endColumn":66},{"ruleId":"120","severity":1,"message":"121","line":98,"column":11,"nodeType":"122","messageId":"123","endLine":98,"endColumn":32},{"ruleId":"112","severity":1,"message":"124","line":64,"column":8,"nodeType":"114","endLine":64,"endColumn":10,"suggestions":"125"},{"ruleId":"126","severity":1,"message":"1